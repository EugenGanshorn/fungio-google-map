{
    "name": "fungio/google-map",
    "description": "Google Map API v3 integration for PHP 5.3.0+",
    "keywords": [ "google map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Pierrick AUBIN",
            "email": "pierrick.aubin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "egeloen/json-builder": "3.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "widop/http-adapter": "~1.1.0",
        "willdurand/geocoder": "~2.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fungio/ivory-json-builder"
        }
    ],
    "suggest": {
        "widop/http-adapter": "Allows to use services",
        "willdurand/geocoder": "Allows to use geocoding service"
    },
    "autoload": {
        "psr-4": { "Fungio\\GoogleMap\\": "src/" }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
